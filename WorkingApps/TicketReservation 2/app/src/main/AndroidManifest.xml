<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.medha.ticketreservation">

    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_flight_black_24dp"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
                <action android:name="android.intent.action.VIEW"/>
                <category android:name="android.intent.category.DEFAULT"/>

            </intent-filter>
        </activity>
        <activity
            android:name=".DeleteActivity"
            android:label="@string/del_activity" />
        <activity
            android:name=".CreateActivity"
            android:label="@string/var_labelcreateactivity"
            android:noHistory="true"/>
        <activity
            android:name=".EditActivity"
            android:label="@string/var_lableEditActivity" />
        <activity
            android:name=".ViewActivity"
            android:label="@string/var_view_Ticket" />
        <activity
            android:name=".PrintActivity"
            android:label="@string/var_label_printTkt">

        </activity>
    </application>

</manifest>
